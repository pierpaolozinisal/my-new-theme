			"scope": [
				"entity.name.import",
				"entity.name.package"
			],
			"settings": {
				"foreground": "#c2d94c"
			}
		},
		{
			"name": "Entity name",
			"scope": [
				"entity.name"
			],
			"settings": {
				"foreground": "#59c2ff"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml"
			],
			"settings": {
				"foreground": "#39bae6"
			}
		},
		{
			"name": "Tag start/end",
